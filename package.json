{
  "name": "wechat-blog",
  "version": "1.0.0",
  "private": true,
  "description": "sRect的个人博客",
  "templateInfo": {
    "name": "default",
    "typescript": false,
    "css": "less"
  },
  "scripts": {
    "prebuild:weapp": "cross-env GEN_TYPE_ENV=json node ./scripts/posts",
    "build:weapp": "cross-env DEV_MODE_ENV=false taro build --type weapp",
    "postbuild:weapp": "node ./scripts/miniprogram.js build && node ./scripts/copyImgFile.js",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "predev:weapp": "echo -e '\u001b[32m工程启动后会自动打开微信开发者工具,如果自动打开失败,请手动打开开发者工具\u001b[0m' && node scripts/miniprogram.js dev",
    "dev:weapp": "cross-env NODE_ENV=production DEV_MODE_ENV=true taro build --type weapp --watch",
    "postdev:weapp": "node ./scripts/copyImgFile.js",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@taroify/core": "^0.1.0-alpha.1",
    "@tarojs/components": "3.6.4",
    "@tarojs/plugin-framework-react": "^3.6.4",
    "@tarojs/plugin-html": "^3.4.3",
    "@tarojs/react": "3.6.4",
    "@tarojs/runtime": "3.6.4",
    "@tarojs/taro": "3.6.4",
    "antd-mobile": "^5.29.1",
    "antd-mobile-icons": "^0.2.2",
    "github-markdown-css": "^5.1.0",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.5.0",
    "immer": "^9.0.12",
    "nanoid": "^3.3.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rehype-stringify": "^9.0.3",
    "remark": "^14.0.2",
    "remark-extended-table": "^1.0.0",
    "remark-gfm": "^3.0.1",
    "remark-html": "^15.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "use-immer": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.6.4",
    "@tarojs/plugin-react-devtools": "^3.4.3",
    "@tarojs/webpack-runner": "3.6.4",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "babel-plugin-import": "^1.13.6",
    "babel-preset-taro": "3.6.4",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.6.4",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "less": "^4.1.3",
    "miniprogram-automator": "^0.12.0",
    "postcss": "^8.4.21",
    "postcss-css-var-to-less-var": "^1.0.0",
    "shelljs": "^0.8.5",
    "stylelint": "^15.4.0",
    "terser-webpack-plugin": "4.2.3",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
